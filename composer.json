{
    "name": "antonioprimera/laravel-activity-log",
    "description": "A complex activity log, saved in a dedicated database.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro"
        }
    ],
    "require-dev": {
        "antonioprimera/lara-packager": "^1.5",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\ActivityLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\ActivityLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\ActivityLog\\ActivityLogServiceProvider"
            ]
        }
    },
  "require": {
    "ext-json": "*"
  }
}